<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
 <template id="website_branding.website_copy_rights_layout"  inherit_id="website.layout_footer_copyright" name="Footer Copyright">
       
    </template>
    <template id="theme_footer" inherit_id="website.layout" customize_show="True" name="Theme Footer" active="True">
      <xpath expr="//div[@id='footer']/.." position="after">
        <!-- ==== Footer Section Start ==== -->
        <footer class="footer-section fix" id="footer">
         
        </footer>
        <!-- ==== Footer Section End ==== -->
      </xpath>
      <xpath expr="//div[@id='footer']/.." position="attributes">
        <attribute name="class">hidden</attribute>
      </xpath>
    </template>
    <template id="website.layout_footer_copyright" inherit_id="website.layout" name="Footer Copyright">
        <xpath expr="//footer" position="inside">
        </xpath>
    </template>
  </data>
</odoo>