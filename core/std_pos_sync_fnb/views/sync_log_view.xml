        <record id="floor_sync_log_form_view" model="ir.ui.view">
			<field name="name">floor.sync.log.form.view</field>
			<field name="model">floor.sync.log</field>
			<field name="arch" type="xml">
				<form string="Restaurant Floor Sync Log">
