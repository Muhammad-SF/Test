<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="restaurant_floor_auditlog_rule" model="auditlog.rule">
            <field name="name">Restaurant Floor Auditlog Rule</field>
            <field name="model_id" search="[('model','=','restaurant.floor')]"/>
            <field name="log_type">full</field>
            <field name="log_write" eval="True"/>
            <field name="log_create" eval="True"/>
            <field name="state">subscribed</field>
        </record>
        
        <record id="restaurant_table_auditlog_rule" model="auditlog.rule">
            <field name="name">Restaurant Table Auditlog Rule</field>
            <field name="model_id" search="[('model','=','restaurant.table')]"/>
            <field name="log_type">full</field>
            <field name="log_write" eval="True"/>
            <field name="log_create" eval="True"/>
            <field name="state">subscribed</field>
        </record>
        
    </data>
</odoo>
